#gamecontainer {
    position: relative;
    width: 773px;
    height: 468px;
    pointer-events: none;
}

.game-element {
    position: absolute;
}

#spritescontainer {
    position: absolute;
    left: 207px;
    top: 114px;
}

#spritescontainer .game-element {
    opacity: 0.05;
}

#spritescontainer .enabled {
    opacity: 1;
}

#container {
    width: 773px;
    height: 468px;
}

#controlscontainer .game-element {
    cursor: pointer;
}

#console {
    height: 80px;
    font-family: Arial;
    font-size: 13px;
}