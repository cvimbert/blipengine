<!DOCTYPE html>
<html>

    <title>LCD Game !</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- underscore templates -->
    <script id="graphic-elem-template" type="text/html">
        <div id="<%= name %>" class="game-element" style="left:<%= x %>px;top:<%= y %>px">
            <img src="img/<%= folder %>/<%= type %>.png">
        </div>
    </script>

    <link href="css/game.css" rel="stylesheet" type="text/css"/>
    <script src="js/jquery-1.11.2.js" type="text/javascript"></script>
    <script src="js/controller.js" type="text/javascript"></script>
    <script src="js/gamemanager.js" type="text/javascript"></script>
    <script src="js/data.js" type="text/javascript"></script>
    <script src="js/layoutmanager.js" type="text/javascript"></script>
    <script src="js/console.js" type="text/javascript"></script>
    <script src="js/underscore.js" type="text/javascript"></script>

    <body>
        <div id="container">
            <div id="gamecontainer">
                <div id="backgroundcontainer">

                </div>
                <div id="spritescontainer">

                </div>
                <div id="foregroundcontainer">

                </div>
                <div id="controlscontainer">

                </div>
            </div>
            <div id="console">
                <div id="console-title">
                    Console:
                </div>
                <div id="console-prompt" contenteditable>
                    
                </div>
                <div id="console-history">
                    
                </div>
            </div>
        </div>
    </body>
</html>
